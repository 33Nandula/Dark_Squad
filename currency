class money
{
    public static void main(String[] args)
    {
        String str1=args[0];
        double num=Double.parseDouble(str1);
        double dollar,pound,euro;
        dollar=(num/129.26);
        pound=(num/132.74);
        euro=(num/206.36);
        System.out.println(num+" LKR "+dollar+" LKR");
        System.out.println(num+" LKR "+pound+" LKR");
        System.out.println(num+" LKR "+euro+" LKR");

    }
}
